<Window x:Class="PastaTime.Check"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PastaTime"
        mc:Ignorable="d"
        Title="Check" Height="450" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="64*"/>
            <RowDefinition Height="355*"/>
        </Grid.RowDefinitions>
        <TextBlock  Grid.Row="0" Foreground="Black" 
                       HorizontalAlignment="Center" VerticalAlignment="Top" FontSize="35" >ЧЕК
        </TextBlock>
        <TextBlock x:Name="Checks"  Grid.Row="1" Foreground="Black" 
                       HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="15">
        </TextBlock>

    </Grid>
</Window>
